{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "@components/*": ["./src/components/*"],
      "@assets/*": ["./src/assets/*"],
      "@scss/*": ["./src/assets/scss/*"],
      "@pages/*": ["./src/pages/*"],
      "@stores/*": ["./src/stores/*"],
      "@customHooks/*": ["./src/customHooks/*"],
      "@layouts/*": ["./src/layouts/*"],
      "@router/*": ["./src/router/*"]
    }
  },
  "exclude": ["node_modules", "dist", "build"]
}
